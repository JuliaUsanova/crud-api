{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "description": "My webpack project",
  "author": "JuliaUsanova",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest --silent",
    "test:verbose": "jest",
    "build:dev": "cross-env NODE_ENV=production webpack --mode=development --watch && node ./dist/main.js",
    "build:prod": "cross-env NODE_ENV=production webpack --mode=production && node ./dist/main.js",
    "debug": "webpack && node --inspect-brk=9229 ./dist/main.js"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@webpack-cli/generators": "^3.0.7",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.2",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "nodemon": "^3.0.3",
    "prettier": "^2.8.8",
    "source-map-loader": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4"
  }
}
